<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B2B48888"
    app:layout_wrapBehaviorInParent="horizontal_only"
    tools:context=".MainActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_55sdp"
        android:background="#CA4A4A"
        android:minHeight="?attr/actionBarSize"
        android:tag="Tic Tac Toe"
        android:textAlignment="viewStart"
        android:theme="?attr/actionBarTheme"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:logo="@drawable/ic_launcher"
        app:subtitleTextAppearance="@style/TextAppearance.AppCompat.Display1"
        app:subtitleTextColor="#2E9932"
        app:title="TicTacToe"
        app:titleTextAppearance="@style/TextAppearance.AppCompat.Display1"
        app:titleTextColor="#0E0E09" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/celebratelayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:lottie_rawRes="@raw/cele" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="@dimen/_404sdp"
        android:layout_height="@dimen/_449sdp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.546">

        <TableRow
            android:layout_width="416dp"
            android:layout_height="112dp"
            android:gravity="center">


            <Button
                android:id="@+id/b1"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_80sdp"
                android:fontFamily="casual"
                android:onClick="result"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textSize="@dimen/_30ssp"
                android:textStyle="bold"
                app:strokeColor="#007D3030"
                tools:ignore="UsingOnClickInXml,SpeakableTextPresentCheck" />

            <Button
                android:id="@+id/b2"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_80sdp"
                android:fontFamily="casual"

                android:onClick="result"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textSize="@dimen/_30ssp"
                android:textStyle="bold"
                tools:ignore="UsingOnClickInXml,SpeakableTextPresentCheck,VisualLintBounds" />

            <Button
                android:id="@+id/b3"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_80sdp"
                android:contentDescription="@string/app_name"

                android:fontFamily="casual"
                android:onClick="result"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textSize="@dimen/_30ssp"
                android:textStyle="bold"
                tools:ignore="UsingOnClickInXml,DuplicateSpeakableTextCheck,VisualLintBounds" />
        </TableRow>

        <TableRow
            android:layout_width="424dp"
            android:layout_height="115dp"
            android:gravity="center">

            <Button
                android:id="@+id/b4"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_80sdp"
                android:fontFamily="casual"

                android:onClick="result"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textSize="@dimen/_30ssp"
                android:textStyle="bold"
                tools:ignore="UsingOnClickInXml,SpeakableTextPresentCheck,VisualLintBounds" />

            <Button
                android:id="@+id/b5"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_80sdp"
                android:fontFamily="casual"

                android:onClick="result"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textSize="@dimen/_30ssp"
                android:textStyle="bold"
                tools:ignore="UsingOnClickInXml,SpeakableTextPresentCheck,SpeakableTextPresentCheck,VisualLintBounds" />

            <Button
                android:id="@+id/b6"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_80sdp"
                android:fontFamily="casual"

                android:onClick="result"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textSize="@dimen/_30ssp"
                android:textStyle="bold"
                tools:ignore="UsingOnClickInXml,SpeakableTextPresentCheck,VisualLintBounds" />


        </TableRow>

        <TableRow
            android:layout_width="418dp"
            android:layout_height="108dp"
            android:gravity="center">

            <Button
                android:id="@+id/b7"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_80sdp"
                android:fontFamily="casual"

                android:onClick="result"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textSize="@dimen/_30ssp"
                android:textStyle="bold"
                tools:ignore="UsingOnClickInXml,SpeakableTextPresentCheck,VisualLintBounds" />

            <Button
                android:id="@+id/b8"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_80sdp"
                android:contentDescription="@string/app_name"
                android:fontFamily="casual"

                android:onClick="result"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textSize="@dimen/_30ssp"
                android:textStyle="bold"
                tools:ignore="UsingOnClickInXml,VisualLintBounds" />

            <Button
                android:id="@+id/b9"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_80sdp"
                android:fontFamily="casual"
                android:onClick="result"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textSize="@dimen/_30ssp"
                android:textStyle="bold"
                tools:ignore="UsingOnClickInXml,SpeakableTextPresentCheck,VisualLintBounds" />

        </TableRow>

    </TableLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="83dp"
        android:layout_height="16dp"
        android:text="@string/volume_on"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977" />

</androidx.constraintlayout.widget.ConstraintLayout>